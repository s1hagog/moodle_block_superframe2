{{!
    @template block_superframe/frame

    This template renders a url in an iframe of given size.

    Example context (json):
    {
        "linkdata": [
            {
                "link": "http://example.com",
                "text": "Go to smth"
            },
            {
                "link": "http://google.com",
                "text": "Go to google"
            }
        ],
    }
}}
<div  class="container">
    <ul class="list-inline">
    {{#linkdata}}
        <li class="list-inline-item">
            <a href="{{{link}}}">{{text}}</a>
        </li>
    {{/linkdata}}
    </ul>
</div>